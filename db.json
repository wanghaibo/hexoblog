lpers/jquery.fancybox-thumbs.css","path":"fancybox/helpers/jquery.fancybox-thumbs.css"},"clxch7bywvx8lgcp":{"_id":"clxch7bywvx8lgcp","source":"themes/pacman/source/fancybox/helpers/jquery.fancybox-thumbs.js","mtime":1400166033000,"modified":false,"full_source":"/Users/wanghaibo/node/blog/themes/pacman/source/fancybox/helpers/jquery.fancybox-thumbs.js","path":"fancybox/helpers/jquery.fancybox-thumbs.js"}},"Cache":{"ieettm5ass3c4sp5":{"_id":"ieettm5ass3c4sp5","content":"title: '开始写点东西'\ndate: 2014-05-15 22:53:56\ntags: 心情 \n---\n#起因\n- 日子过得挺浮躁的，想写点东西记录一下生活\n\n#todo\n- 遇到的心情可以记录下来，后来回味起来别有一番风味\n- 写一下读书笔记，书经常在看，有得时候发现想几点东西也不方便，这下有地方了\n- 写一些技术总结\n- 国外有一牛人每天早上5点到8点写博文，坚持了3年，本来想效仿一下，但是肚子里料没人家多，先计划一周写一篇\n","mtime":1401705716000,"source":"source/_posts/first-post.md"},"kfi2hyk6r9tkn3bl":{"_id":"kfi2hyk6r9tkn3bl","content":"<%- partial('_partial/archive', {pagination: config.archive}) %>","mtime":1400166033000,"source":"themes/pacman/layout/archive.ejs"},"mvz7h88sjxih2cep":{"_id":"mvz7h88sjxih2cep","content":"<%- partial('_partial/archive', {pagination: config.category}) %>","mtime":1400166033000,"source":"themes/pacman/layout/category.ejs"},"tyy1dn4pzdg7ns6f":{"_id":"tyy1dn4pzdg7ns6f","content":"<div id=\"main\">\n<% page.posts.each(function(item){ %>\n  <%- partial('_partial/article', {item: item, index: true,desc: true}) %>\n<% }); %>\n<% if (page.total > 1){ %>\n  <nav id=\"page-nav\" class=\"clearfix\">\n    <%- paginator({\n      prev_text: '&laquo; Prev',\n      next_text: 'Next &raquo;'\n    }) %>\n  </nav>\n<% } %>\n</div>","mtime":1400166033000,"source":"themes/pacman/layout/index.ejs"},"z0gj3zcl87o56mzx":{"_id":"z0gj3zcl87o56mzx","content":"<% if (page.layout=='post'){ %>\n <%- partial('_partial/head') %>\n  <body>\n    <header>\n      <%- partial('_partial/header') %>\n    </header>\n    <div id=\"container\">\n      <%- body %>\n      <%- partial('_partial/sidebar',{item: page,table: true}) %>\n    </div>\n    <footer><%- partial('_partial/footer') %></footer>\n    <%- partial('_partial/after_footer') %>\n  </body>\n</html>\n<% } else if(page.layout=='page'){ %>\n  <% if(page.source.match(/\\.md$/)){ %>\n    <%- partial('_partial/head') %>\n      <body>\n        <header>\n          <%- partial('_partial/header') %>\n        </header>\n        <div id=\"container\">\n          <%- body %>\n        </div>\n        <footer><%- partial('_partial/footer') %></footer>\n        <%- partial('_partial/after_footer') %>\n      </body>\n     </html>\n     <% }else{ %>\n    <%- page.content %>\n  <% } %>\n<% } else if(page.layout=='search'){ %>\n <%- partial('_partial/head') %>\n    <body>\n      <header>\n        <%- partial('_partial/header') %>\n      </header>\n      <div id=\"container\">\n        <%- partial('_partial/search')%>\n      </div>\n      <%- partial('_partial/after_footer') %>\n    </body>\n   </html>\n<% } else if(page.layout=='tags'){ %>\n <%- partial('_partial/head') %>\n    <body>\n      <header>\n        <%- partial('_partial/header') %>\n      </header>\n      <div id=\"container\">\n          <%- partial('_partial/tags')%>\n      </div>\n      <%- partial('_partial/after_footer') %>\n    </body>\n   </html>\n<% } else if(page.layout=='categories'){ %>\n <%- partial('_partial/head') %>\n    <body>\n      <header>\n        <%- partial('_partial/header') %>\n      </header>\n      <div id=\"container\">\n          <%- partial('_partial/categories')%>\n      </div>\n      <%- partial('_partial/after_footer') %>\n    </body>\n   </html>\n<% } else if(page.category!=null||page.tag!=null||page.archive!=null) { %>\n  <%- partial('_partial/head') %>\n    <body>\n      <header>\n        <%- partial('_partial/header') %>\n      </header>\n      <div id=\"container\">\n        <%- body %>\n      </div>\n      <footer><%- partial('_partial/footer') %></footer>\n      <%- partial('_partial/after_footer') %>\n    </body>\n   </html>\n<% } else { %>\n <%- partial('_partial/head') %>\n  <body>\n    <header>\n      <%- partial('_partial/header') %>\n    </header>\n    <div id=\"container\">\n      <%- body %>\n      <%- partial('_partial/sidebar',{item: page,table: false}) %>\n    </div>\n    <footer><%- partial('_partial/footer') %></footer>\n    <%- partial('_partial/after_footer') %>\n  </body>\n </html>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/layout.ejs"},"vx5fpl19efe6gln8":{"_id":"vx5fpl19efe6gln8","content":"<%- partial('_partial/post/article', {item: page, index: false,table: false}) %>","mtime":1400166033000,"source":"themes/pacman/layout/page.ejs"},"l2jhmkzppe4n3f9i":{"_id":"l2jhmkzppe4n3f9i","content":"<%- partial('_partial/post/article', {item: page, index: false,table: true}) %>","mtime":1400166033000,"source":"themes/pacman/layout/post.ejs"},"i2o49pmwcgc639zg":{"_id":"i2o49pmwcgc639zg","content":"<%- partial('_partial/archive', {pagination: config.tag}) %>","mtime":1400166033000,"source":"themes/pacman/layout/tag.ejs"},"ksadj4k1tv128b81":{"_id":"ksadj4k1tv128b81","content":"<% if (site.posts.length){ %>\n  <div class=\"archiveslist\">\n    <p class=\"asidetitle\"><a href=\"<%- config.root %>archives\"><%= __('archive_a') %></a></p>\n      <%- list_archives() %>\n  </div>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_widget/archive.ejs"},"s7zf06fhjx3ms1ff":{"_id":"s7zf06fhjx3ms1ff","content":"<% if (site.categories.length){ %>\n<div class=\"categorieslist\">\n\t<p class=\"asidetitle\"><%= __('categories') %></p>\n\t\t<ul>\n\t\t<% site.categories.sort('name').each(function(item){ %>\n\t\t\t<li><a href=\"<%- config.root %><%- item.path %>\" title=\"<%= item.name %>\"><%= item.name %><sup><%= item.posts.length %></sup></a></li>\n\t\t<% }); %>\n\t\t</ul>\n</div>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_widget/category.ejs"},"oraunm9hfehyjbey":{"_id":"oraunm9hfehyjbey","content":"<div class=\"linkslist\">\n  <p class=\"asidetitle\"><%= __('links') %></p>\n    <ul>\n      <li><a href=\"http://yangjian.me\" target=\"_blank\" title=\"YangJian\">Alimon's Blog</a></li>\n      <li><a href=\"http://zespia.tw/hexo\" target=\"_blank\" title=\"Hexo\">Hexo</a></li>\n    </ul>\n</div>","mtime":1400166033000,"source":"themes/pacman/layout/_widget/links.ejs"},"9t93qbttshnn23k5":{"_id":"9t93qbttshnn23k5","content":"<div class=\"rsspart\">\n\t<a href=\"<%= theme.rss %>\" target=\"_blank\" title=\"rss\"><%= __('rss') %></a>\n</div>","mtime":1400166033000,"source":"themes/pacman/layout/_widget/rss.ejs"},"ihmfrkdzd8k9zyo0":{"_id":"ihmfrkdzd8k9zyo0","content":"<% if (site.tags.length){ %>\n<div class=\"tagslist\">\n\t<p class=\"asidetitle\"><%= __('tags') %></p>\n\t\t<ul class=\"clearfix\">\n\t\t<% site.tags.sort('name').limit(20).each(function(item){ %>\n\t\t\t<li><a href=\"<%- config.root %><%- item.path %>\" title=\"<%= item.name %>\"><%= item.name %><sup><%= item.posts.length %></sup></a></li>\n\t\t<% }); %>\n\t\t</ul>\n</div>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_widget/tag.ejs"},"29gzodp6644r33o1":{"_id":"29gzodp6644r33o1","content":"<% if (site.tags.length){ %>\n  <div class=\"tagcloudlist\">\n    <p class=\"asidetitle\"><%= __('tagcloud') %></p>\n    <div class=\"tagcloudlist clearfix\">\n       <%- tagcloud() %>\n    </div>\n  </div>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_widget/tagcloud.ejs"},"1e0ekvut772j1q3x":{"_id":"1e0ekvut772j1q3x","content":"<script src=\"<%- config.root %>js/jquery-2.1.0.min.js\"></script>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  $('.navbar').click(function(){\n    $('header nav').toggleClass('shownav');\n  });\n  var myWidth = 0;\n  function getSize(){\n    if( typeof( window.innerWidth ) == 'number' ) {\n      myWidth = window.innerWidth;\n    } else if( document.documentElement && document.documentElement.clientWidth) {\n      myWidth = document.documentElement.clientWidth;\n    };\n  };\n  var m = $('#main'),\n      a = $('#asidepart'),\n      c = $('.closeaside'),\n      o = $('.openaside');\n  $(window).resize(function(){\n    getSize(); \n    if (myWidth >= 1024) {\n      $('header nav').removeClass('shownav');\n    }else\n    {\n      m.removeClass('moveMain');\n      a.css('display', 'block').removeClass('fadeOut');\n      o.css('display', 'none');\n      <% if( is_post()&&(page.toc !== false) && theme.toc.aside){ %>\n      $('#toc.toc-aside').css('display', 'none');\n      <% } %>  \n    }\n  });\n  c.click(function(){\n    a.addClass('fadeOut').css('display', 'none');\n    o.css('display', 'block').addClass('fadeIn');\n    m.addClass('moveMain');\n  });\n  o.click(function(){\n    o.css('display', 'none').removeClass('beforeFadeIn');\n    a.css('display', 'block').removeClass('fadeOut').addClass('fadeIn');      \n    m.removeClass('moveMain');\n  });\n  $(window).scroll(function(){\n    o.css(\"top\",Math.max(80,260-$(this).scrollTop()));\n  });\n});\n</script>\n<% if (is_post()) { %>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  var ai = $('.article-content>iframe'),\n      ae = $('.article-content>embed'),\n      t  = $('#toc'),\n      h  = $('article h2')\n      ah = $('article h2'),\n      ta = $('#toc.toc-aside'),\n      o  = $('.openaside'),\n      c  = $('.closeaside');\n  if(ai.length>0){\n    ai.wrap('<div class=\"video-container\" />');\n  };\n  if(ae.length>0){\n   ae.wrap('<div class=\"video-container\" />');\n  };\n  if(ah.length==0){\n    t.css('display','none');\n  }else{\n    c.click(function(){\n      ta.css('display', 'block').addClass('fadeIn');\n    });\n    o.click(function(){\n      ta.css('display', 'none');\n    });\n    $(window).scroll(function(){\n      ta.css(\"top\",Math.max(140,320-$(this).scrollTop()));\n    });\n  };\n});\n</script>\n<% } %>\n<% if ((page.layout=='post'||page.layout=='page')&&!theme.jiathis.enable) { %>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  var $this = $('.share'),\n      url = $this.attr('data-url'),\n      encodedUrl = encodeURIComponent(url),\n      title = $this.attr('data-title'),\n      tsina = $this.attr('data-tsina');\n  var html = [\n  '<a href=\"#\" class=\"overlay\" id=\"qrcode\"></a>',\n  '<div class=\"qrcode clearfix\"><span>扫描二维码分享到微信朋友圈</span><a class=\"qrclose\" href=\"#share\"></a><strong>Loading...Please wait</strong><img id=\"qrcode-pic\" data-src=\"http://s.jiathis.com/qrcode.php?url=' + encodedUrl + '\"/></div>',\n  '<a href=\"#textlogo\" class=\"article-back-to-top\" title=\"Top\"></a>',\n  '<a href=\"https://www.facebook.com/sharer.php?u=' + encodedUrl + '\" class=\"article-share-facebook\" target=\"_blank\" title=\"Facebook\"></a>',\n  '<a href=\"#qrcode\" class=\"article-share-qrcode\" title=\"QRcode\"></a>',\n  '<a href=\"https://twitter.com/intent/tweet?url=' + encodedUrl + '\" class=\"article-share-twitter\" target=\"_blank\" title=\"Twitter\"></a>',\n  '<a href=\"http://service.weibo.com/share/share.php?title='+title+'&url='+encodedUrl +'&ralateUid='+ tsina +'&searchPic=true&style=number' +'\" class=\"article-share-weibo\" target=\"_blank\" title=\"Weibo\"></a>',\n  '<span title=\"Share to\"></span>'\n  ].join('');\n  $this.append(html);\n  $('.article-share-qrcode').click(function(){\n    var imgSrc = $('#qrcode-pic').attr('data-src');\n    $('#qrcode-pic').attr('src', imgSrc);\n    $('#qrcode-pic').load(function(){\n        $('.qrcode strong').text(' ');\n    });\n  });\n});     \n</script>\n<% } %>\n<% if ((page.layout=='post'||page.layout=='page')&&theme.duoshuo.enable && page.comments){ %>\n<script type=\"text/javascript\">\n  var duoshuoQuery = {short_name:\"<%= theme.duoshuo.short_name %>\"};\n  (function() {\n    var ds = document.createElement('script');\n    ds.type = 'text/javascript';ds.async = true;\n    ds.src = '//static.duoshuo.com/embed.js';\n    ds.charset = 'UTF-8';\n    (document.getElementsByTagName('head')[0] \n    || document.getElementsByTagName('body')[0]).appendChild(ds);\n  })();\n</script> \n<% } %>\n<% if (page.layout=='tags'||page.layout=='categories'){ %>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  var list    = $('.archive-title a'),\n      box     = $('.all-list-box'),\n      first   = $('.archive-title a:first-of-type');\n      box.load(first.attr('data-src').toString()+' #archive-page');\n      first.addClass('current');\n  list.each(function(){\n    $(this).click(function(){\n      var listSrc=$(this).attr('data-src').toString()+' #archive-page';\n      box.load(listSrc);\n      list.removeClass('current');\n      $(this).addClass('current');\n     });  \n    });\n  }); \n</script>\n<% } %>\n<% if ((page.layout=='post'||page.layout=='page')&&theme.fancybox){ %>\n<link rel=\"stylesheet\" href=\"<%- config.root %>fancybox/jquery.fancybox.css\" media=\"screen\" type=\"text/css\">\n<script src=\"<%- config.root %>fancybox/jquery.fancybox.pack.js\"></script>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  $('.article-content').each(function(i){\n    $(this).find('img').each(function(){\n      if ($(this).parent().hasClass('fancybox')) return;\n      var alt = this.alt;\n      if (alt) $(this).after('<span class=\"caption\">' + alt + '</span>');\n      $(this).wrap('<a href=\"' + this.src + '\" title=\"' + alt + '\" class=\"fancybox\"></a>');\n    });\n    $(this).find('.fancybox').each(function(){\n      $(this).attr('rel', 'article' + i);\n    });\n  });\n  if($.fancybox){\n    $('.fancybox').fancybox();\n  }\n}); \n</script>\n<% } %>\n<%- partial('google_analytics') %>\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/after_footer.ejs"},"g0j7a50c39f3ui83":{"_id":"g0j7a50c39f3ui83","content":"<%\nvar title = '';\nvar icon = '';\nif (page.category){\n  title = page.category;\n  icon = 'category';\n};\nif (page.tag){\n  title = page.tag;\n  icon = 'tag';\n};\nif (page.archive){\n  icon = 'archive';\n  if (page.year) title = page.year+  (page.month ? '/' + page.month : '');\n  else title = __('archive_a');\n};\n%>\n\n<div class=\"archive-title\" >\n  <h2 class=\"<%= icon %>-icon\"><%= title %></h2>\n  <% if(page.archive){ %>\n  <div class=\"archiveslist archive-float clearfix\">\n   <%- list_archives() %>\n </div>\n  <% } %>\n</div>\n<div id=\"main\" class=\"archive-part clearfix\">\n  <div id=\"archive-page\">\n<% page.posts.each(function(item){ %>\n  <%- partial('_partial/article', {item: item, index: true,desc: false}) %>\n<% }); %>\n<% if (page.total > 1){ %>\n  <nav id=\"page-nav\" class=\"clearfix archive-nav\">\n    <%- paginator({\n      prev_text: '&laquo; Prev',\n      next_text: 'Next &raquo;'\n    }) %>\n  </nav>\n<% } %>\n  </div>\n</div>\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/archive.ejs"},"vowj3o2n2hnk7plb":{"_id":"vowj3o2n2hnk7plb","content":"<section class=\"<%= item.layout %>\" itemscope itemprop=\"blogPost\">\n  <% if (item.link) { %> \n    <a href=\"<%- item.link %>\" target=\"_blank\"> \n  <% } else{ %>\n    <a href=\"<%- config.root %><%- item.path %>\" title=\"<%= item.title %>\" itemprop=\"url\">\n  <% } %>\n    <h1 itemprop=\"name\"><%= item.title %></h1>\n    <% if (desc){ %>\n     <% if(item.description){ %>\n      <p itemprop=\"description\" ><%- item.description %></p>\n      <% } else if(item.excerpt){ %>\n       <p itemprop=\"description\" ><%= strip_html(item.excerpt).replace(/^\\s*/, '').replace(/\\s*$/, '').substring(0, 140) %></p>\n      <% } else {%>\n           <p itemprop=\"description\" ><%= strip_html(item.content).replace(/^\\s*/, '').replace(/\\s*$/, '').substring(0, 140) %></p>\n        <% } %>\n    <% } %>\n    <time datetime=\"<%= date_xml(item.date) %>\" itemprop=\"datePublished\"><%= item.date.format(config.date_format) %></time>\n  </a>\n</section>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/article.ejs"},"sq14g9987jdl41li":{"_id":"sq14g9987jdl41li","content":"<%\npage.category = ' ';\n%>\n<div class=\"archive-title\" >\n<h2 class=\"category-icon\"><%= __('categories') %></h2>\n<% site.categories.sort('name').each(function(item){ %>\n<a data-src=\"<%- config.root %><%- item.path %>\" href=\"#<%= item.name %>\" title=\"<%= item.name %>\"><%= item.name %><sup><%= item.posts.length %></sup></a>\n<% }); %>\n</div>\n<div id=\"main\" class=\"archive-part\">\n<div class=\"all-list-box\"></div>\n</div>\n\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/categories.ejs"},"t17t8gc3vew6qo8h":{"_id":"t17t8gc3vew6qo8h","content":"<div id=\"footer\" >\n\t<% if(theme.author_img_enable == true){ %>\n\t<div class=\"line\">\n\t\t<span></span>\n\t\t<div class=\"author\"></div>\n\t</div>\n\t<% } %>\n\t<% if(theme.author.intro_line1&&theme.author.intro_line2){ %>\n\t<section class=\"info\">\n\t\t<p> <%= theme.author.intro_line1 %> <br/>\n\t\t\t<%= theme.author.intro_line2 %></p>\n\t</section>\n\t <% } %>\n\t<div class=\"social-font\" class=\"clearfix\">\n\t\t<% if(theme.author.weibo){ %>\n\t\t<a href=\"http://weibo.com/<%= theme.author.weibo %>\" target=\"_blank\" title=\"weibo\"></a>\n\t\t<% } %>\n\t\t<% if(theme.author.twitter){ %>\n\t\t<a href=\"https://twitter.com/<%=theme.author.twitter %>\" target=\"_blank\" title=\"twitter\"></a>\n\t\t<% } %>\n\t\t<% if(theme.author.github){ %>\n\t\t<a href=\"https://github.com/<%=theme.author.github %>\" target=\"_blank\" title=\"github\"></a>\n\t\t<% } %>\n\t\t<% if(theme.author.facebook){ %>\n\t\t<a href=\"https://www.facebook.com/<%=theme.author.facebook %>\" target=\"_blank\" title=\"facebook\"></a>\n\t\t<% } %>\n\t</div>\n\t\t<p class=\"copyright\">Powered by <a href=\"http://zespia.tw/hexo/\" target=\"_blank\" title=\"hexo\">hexo</a> and Theme by <a href=\"https://github.com/A-limon/pacman\" target=\"_blank\" title=\"Pacman\">Pacman</a> © <%= new Date().getFullYear() %> \n\t\t<% if (config.author) { %>\n\t\t<a href=\"<%= config.url %>\" target=\"_blank\" title=\"<%= config.author %>\"><%= config.author %></a>\n\t\t<% } else {%>\n\t\t<a href=\"<%= config.url %>\" title=\"<%= config.title %>\"><%= config.title %></a>\n\t\t<% } %>\n\t\t</p>\n</div>\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/footer.ejs"},"rnuvj95bvrfpboc0":{"_id":"rnuvj95bvrfpboc0","content":"<% if (theme.google_analytics.enable){ %>\n<script type=\"text/javascript\">\n(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){\n  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),\n  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)\n})(window,document,'script','//www.google-analytics.com/analytics.js','ga');\nga('create', '<%= theme.google_analytics.id %>', '<%= theme.google_analytics.site %>');  \nga('send', 'pageview');\n</script>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/google_analytics.ejs"},"e4613glzqk0c67gi":{"_id":"e4613glzqk0c67gi","content":"<!DOCTYPE HTML>\n<html <% if (config.language) { %>lang=\"<%= config.language %>\"<% } %>>\n<head>\n  <meta charset=\"UTF-8\">\n  <%\n  var title = page.title;\n\n  if (is_archive()){\n    title = 'Archives';\n\n    if (is_month()){\n      title += ': ' + page.year + '/' + page.month;\n    } else if (is_year()){\n      title += ': ' + page.year;\n    }\n  } else if (is_category()){\n    title = 'Category: ' + page.category;\n  } else if (is_tag()){\n    title = 'Tag: ' + page.tag;\n  }\n  %>\n    <title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=3, minimum-scale=1\">\n    <% if (config.author){ %>\n    <meta name=\"author\" content=\"<%= config.author %>\"><% } %>\n    <% if (page.description){ %>\n    <meta name=\"description\" itemprop=\"description\" content=\"<%= page.description %>\">\n    <% } else if (config.description&&(!is_post())){ %>\n    <meta name=\"description\" content=\"<%= config.description %>\">\n    <% } else if (page.excerpt){ %>\n    <meta name=\"description\" content=\"<%= strip_html(page.excerpt).replace(/^\\s*/, '').replace(/\\s*$/, '') %>\">\n    <% } else if (page.content){ %>\n    <meta name=\"description\" content=\"<%= strip_html(page.content).replace(/^\\s*/, '').replace(/\\s*$/, '').substring(0, 150) %>\">\n    <% } %>\n    <% if (page.keywords){ %><meta name=\"keywords\" content=\"<%= page.keywords %>\"><% } %>\n    <% if (theme.author.twitter){ %>\n    <meta name=\"twitter:card\" content=\"summary\" />\n    <meta name=\"twitter:site\" content=\"<%= theme.author.twitter %>\" />\n    <meta name=\"twitter:title\" content=\"<% if (title){ %><%= title %> | <% } %><%= config.title %>\" />\n      <% if (page.description){ %>\n        <meta name=\"twitter:description\" content=\"<%= page.description %>\" />\n      <% } %>\n    <% } %>\n    <% if (theme.rss){ %>\n    <link rel=\"alternative\" href=\"<%- theme.rss %>\" title=\"<%= config.title %>\" type=\"application/atom+xml\">\n    <% } %>\n    <% if (theme.favicon){ %>\n    <link rel=\"icon\" href=\"<%- config.root %><%- theme.favicon %>\">\n    <% } %>\n    <% if (theme.apple_icon){ %>\n    <link rel=\"apple-touch-icon\" href=\"<%- config.root %><%- theme.apple_icon %>\">\n    <link rel=\"apple-touch-icon-precomposed\" href=\"<%- config.root %><%- theme.apple_icon %>\">\n    <% } %>\n    <%- css('css/style') %>\n</head>\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/head.ejs"},"tryv01018p9el8c2":{"_id":"tryv01018p9el8c2","content":"<div>\n\t\t<% if\t(theme.imglogo.enable&&theme.imglogo.src){ %>\n\t\t\t<div id=\"imglogo\">\n\t\t\t\t<a href=\"<%- config.root %>\"><img src=\"<%- config.root %><%= theme.imglogo.src %>\" alt=\"<%= config.title %>\" title=\"<%= config.title %>\"/></a>\n\t\t\t</div>\n\t\t\t<% } %>\n\t\t\t<div id=\"textlogo\">\n\t\t\t\t<h1 class=\"site-name\"><a href=\"<%- config.root %>\" title=\"<%= config.title %>\"><%= config.title %></a></h1>\n\t\t\t\t<h2 class=\"blog-motto\"><% if (config.subtitle){ %><%= config.subtitle %><% } %></h2>\n\t\t\t</div>\n\t\t\t<div class=\"navbar\"><a class=\"navbutton navmobile\" href=\"#\" title=\"<%= __('menu') %>\">\n\t\t\t</a></div>\n\t\t\t<nav class=\"animated\">\n\t\t\t\t<ul>\n\t\t\t\t\t<ul>\n\t\t\t\t\t <% for (var i in theme.menu){ %>\n\t\t\t\t\t\t<li><a href=\"<%- theme.menu[i] %>\"><%= i %></a></li>\n\t\t\t\t\t<% } %>\n\t\t\t\t\t<li>\n\t\t\t\t\t<% if\t(theme.google_cse&&theme.google_cse.enable){ %>\n\t\t\t\t\t<form class=\"search\" action=\"<%- config.root %>search/index.html\" method=\"get\" accept-charset=\"utf-8\">\n\t\t\t\t\t\t<label>Search</label>\n\t\t\t\t\t\t<input type=\"text\" id=\"search\" autocomplete=\"off\" name=\"q\" maxlength=\"20\" placeholder=\"<%= __('search') %>\" />\n\t\t\t\t\t</form>\n\t\t\t\t\t<% } else { %>\n\t\t\t\t\t<form class=\"search\" action=\"//google.com/search\" method=\"get\" accept-charset=\"utf-8\">\n\t\t\t\t\t\t<label>Search</label>\n\t\t\t\t\t\t<input type=\"text\" id=\"search\" name=\"q\" autocomplete=\"off\" maxlength=\"20\" placeholder=\"<%= __('search') %>\" />\n\t\t\t\t\t\t<input type=\"hidden\" name=\"q\" value=\"site:<%- config.url.replace(/^https?:\\/\\//, '') %>\">\n\t\t\t\t\t</form>\n\t\t\t\t\t<% } %>\n\t\t\t\t\t</li>\n\t\t\t\t</ul>\n\t\t\t</nav>\t\t\t\n</div>\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/header.ejs"},"n3as2z232rfgdl2v":{"_id":"n3as2z232rfgdl2v","content":"<nav class=\"pagenav clearfix\">\n\t<% if (page.prev){ %>\n\t\t<a href=\"<%- config.root %><%- page.prev_link %>\" title=\"<%= __('prev') %>\" class=\"prevpage\"><%= __('prev') %></a>\n\t<% } %>\n\t<% if (page.next){ %>\n\t\t<a href=\"<%- config.root %><%- page.next_link %>\" title=\"<%= __('next') %>\" class=\"nextpage\"><%= __('next') %></a>\n\t<% } %>\n</nav>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/pagination.ejs"},"01rach914sfbpayj":{"_id":"01rach914sfbpayj","content":"<% if(theme.google_cse.enable) { %>\n<div  id=\"main\" class=\"page\">\n  <div id='cse' style='width:100%;text-align:center'>正在加载搜索结果，请稍等。</div>\n</div>\n<script src=\"http://www.google.com.hk/jsapi\" type=\"text/javascript\"></script>\n<script type=\"text/javascript\">\ngoogle.load('search', '1', {language : 'zh-CN',style: google.loader.themes.MINIMALIST});\ngoogle.setOnLoadCallback(function(){\n  var customSearchControl = new google.search.CustomSearchControl('<%= theme.google_cse.cx %>');\n  customSearchControl.setResultSetSize(google.search.Search.FILTERED_CSE_RESULTSET);\n  var options = new google.search.DrawOptions();\n  options.setAutoComplete(true);\n  customSearchControl.draw('cse', options);\n  var match = location.search.match(/q=([^&]*)(&|$)/);\n  if(match && match[1]){\n    var search = decodeURIComponent(match[1]); \n    customSearchControl.execute(search);\n  }\n},true);\n</script>\n<% } %>\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/search.ejs"},"3qhg9cydp4mrqnji":{"_id":"3qhg9cydp4mrqnji","content":"<div class=\"openaside\"><a class=\"navbutton\" href=\"#\" title=\"<%= __('showsidebar') %>\"></a></div>\n<% if( table&&(item.toc !== false) && theme.toc.aside){ %>\n  <div id=\"toc\" class=\"toc-aside\">\n  <strong class=\"toc-title\"><%= __('contents') %></strong>\n  <%- toc(item.content) %>\n  </div>\n<% } %>\n<div id=\"asidepart\">\n<div class=\"closeaside\"><a class=\"closebutton\" href=\"#\" title=\"<%= __('hidesidebar') %>\"></a></div>\n<aside class=\"clearfix\">\n<% theme.widgets.forEach(function(widget){ %>\n  <%- partial('../_widget/' + widget) %>\n<% }); %>\n</aside>\n</div>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/sidebar.ejs"},"9726usdad2mr9rej":{"_id":"9726usdad2mr9rej","content":"<%\npage.tag = ' ';\n%>\n<div class=\"archive-title\" >\n<h2 class=\"tag-icon\"><%= __('tags') %></h2>\n<% site.tags.sort('name').each(function(item){ %>\n<a data-src=\"<%- config.root %><%- item.path %>\" href=\"#<%= item.name %>\" title=\"<%= item.name %>\"><%= item.name %><sup><%= item.posts.length %></sup></a>\n<% }); %>\n</div>\n<div id=\"main\" class=\"archive-part\">\n<div class=\"all-list-box\"></div>\n</div>\n\n\n\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/tags.ejs"},"m2k4dce0y0kquafj":{"_id":"m2k4dce0y0kquafj","content":"<div id=\"main\" class=\"<%= item.layout %>\" itemscope itemprop=\"blogPost\">\n\t<article itemprop=\"articleBody\"> \n\t\t<%- partial('header') %>\n\t<div class=\"article-content\">\n\t\t<%- partial('gallery') %>\n\t\t<% if( table&&(item.toc !== false) && theme.toc.article){ %>\n\t\t<div id=\"toc\" class=\"toc-article\">\n\t\t\t<strong class=\"toc-title\"><%= __('contents') %></strong>\n\t\t<%- toc(item.content) %>\n\t\t</div>\n\t\t<% } %>\n\t\t<%- item.content %>  \n\t</div>\n\t\t<%- partial('footer') %>   \t       \n\t</article>\n\t<%- partial('pagination') %>\n\t<%- partial('comment') %>\n</div>  ","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/article.ejs"},"3heurgwqojfonj07":{"_id":"3heurgwqojfonj07","content":"<% if (item.tags && item.tags.length){ %>\n  <div class=\"article-tags\">\n  <% var tags = [];\n    item.tags.forEach(function(tag){\n      tags.push('<a href=\"' + config.root + tag.path + '\">' + tag.name + '</a>');\n    }); %>\n  <span></span> <%- tags.join('') %>\n  </div>\n<% } %>\n<% if (item.categories && item.categories.length){ %>\n<div class=\"article-categories\">\n  <span></span>\n  <%- list_categories(item.categories, {\n      show_count: false,\n      class: 'article-category',\n      style: 'none',\n      separator: '►'\n  }) %>\n</div>\n<% } %>\n\n","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/catetags.ejs"},"tje71fsd1snpd1yn":{"_id":"tje71fsd1snpd1yn","content":"<% if (theme.duoshuo.enable && page.comments){ %>\n<section class=\"comment\">\n\t<div class=\"ds-thread\"></div>\n</section>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/comment.ejs"},"m0dx4r4cu9mlf2jo":{"_id":"m0dx4r4cu9mlf2jo","content":"<footer class=\"article-footer clearfix\">\n<%- partial('catetags') %>\n<div class=\"article-share\" id=\"share\">\n<% if(theme.jiathis.enable){ %>\n  <%- partial('jiathis') %>\n<% } else { %>\n  <div data-url=\"<%- item.permalink %>\" data-title=\"<% if (item.title){ %><%= item.title %> | <% } %><%= config.title %>\" data-tsina=\"<%= theme.author.tsina %>\" class=\"share clearfix\">\n  </div>\n<% } %>\n</div>\n</footer>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/footer.ejs"},"k8dkopgsehclk3ga":{"_id":"k8dkopgsehclk3ga","content":"<% if (item.photos && item.photos.length){ %>\n<div class=\"article-gallery\">\n  <div class=\"article-gallery-photos\">\n    <% item.photos.forEach(function(photo, i){ %>\n      <a class=\"article-gallery-img fancybox\" href=\"<%- photo %>\" rel=\"gallery_<%= item._id %>\">\n        <img src=\"<%- photo %>\" itemprop=\"image\">\n      </a>\n    <% }) %>\n  </div>\n</div>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/gallery.ejs"},"vcdrnk9ygvma6aq6":{"_id":"vcdrnk9ygvma6aq6","content":"<header class=\"article-info clearfix\">\n  <h1 itemprop=\"name\">\n    <% if(item.link) { %>\n      <a href=\"<%- item.link %>\" target=\"_blank\" title=\"<%= item.title %>\"><%= item.title %></a><% } else { %>\n      <a href=\"<%- config.root %><%- item.path %>\" title=\"<%= item.title %>\" itemprop=\"url\"><%= item.title %></a><% } %>\n  </h1>\n  <p class=\"article-author\">By\n    <% if (theme.author.google_plus){ %>\n      <a href=\"https://plus.google.com/<%- theme.author.google_plus %>?rel=author\" title=\"<%= config.author %>\" target=\"_blank\" itemprop=\"author\"><%= config.author %></a>\n    <% } else { %>\n      <a href=\"<%= config.url %>\" title=\"<%= config.author %>\"><%= config.author %></a>\n    <% } %></p>\n  <p class=\"article-time\">\n    <time datetime=\"<%= date_xml(item.date) %>\" itemprop=\"datePublished\"><%= item.date.format(config.date_format) %></time>\n    <% if (item.updated!=null) { %><%= __('updated') %>:<time datetime=\"<%= date_xml(item.updated) %>\" itemprop=\"dateModified\"><%= item.updated.format(config.date_format) %></time>\n    <% } %>\n  </p>\n</header>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/header.ejs"},"tgkb7bjd42ky424j":{"_id":"tgkb7bjd42ky424j","content":"<% if (theme.jiathis.enable){ %>\n<div class=\"jiathis_style\">\n    <span class=\"jiathis_txt\">分享到：</span>\n    <a class=\"jiathis_button_tsina\">新浪微博</a>\n    <a class=\"jiathis_button_weixin\">微信</a>\n    <a class=\"jiathis_button_twitter\">Twitter</a>\n    <a class=\"jiathis_button_evernote\">EverNote</a>\n    <a href=\"http://www.jiathis.com/share?uid=1501277\" class=\"jiathis jiathis_txt jiathis_separator jtico jtico_jiathis\" target=\"_blank\">更多</a>\n</div>\n<script type=\"text/javascript\" >\n    var jiathis_config={\n    data_track_clickback:true,\n    sm:\"copy,renren,cqq\",\n    pic:\"<%- item.photos %>\",\n    summary:\"\",\n    <% if (theme.jiathis.tsina){ %> ralateuid:{\"tsina\":\"<%= theme.jiathis.tsina %>\"},hideMore:false}\n    <% } %>\n  </script> \n<script type=\"text/javascript\" src=\"//v3.jiathis.com/code/jia.js?uid=\n<% if (theme.jiathis.id){ %><%= theme.jiathis.id %><% } %>\" charset=\"utf-8\"></script>      \n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/jiathis.ejs"},"0bpaxjqiwfjalhz7":{"_id":"0bpaxjqiwfjalhz7","content":"<% if (page.prev || page.next){ %>\n<nav class=\"article-nav clearfix\">\n <% if (page.prev){ %>\n <div class=\"prev\" >\n <a href=\"<%- config.root %><%- page.prev.path %>\" title=\"<%= page.prev.title %>\">\n  <strong>PREVIOUS:</strong><br/>\n  <span>\n  <% if (page.prev.title){ %><%= page.prev.title %><% } else { %>(no title)<% } %></span>\n</a>\n</div>\n<% } %>\n<% if (page.next){ %>\n<div class=\"next\">\n<a href=\"<%- config.root %><%- page.next.path %>\"  title=\"<%= page.next.title %>\">\n <strong>NEXT:</strong><br/> \n <span><% if (page.next.title){ %><%= page.next.title %><% } else { %>(no title)<% } %>\n</span>\n</a>\n</div>\n<% } %>\n</nav>\n<% } %>","mtime":1400166033000,"source":"themes/pacman/layout/_partial/post/pagination.ejs"},"wfzfbu8a6e1tu3pd":{"_id":"wfzfbu8a6e1tu3pd","content":"title: test\ndate: 2014-05-16 01:19:40\ntags:\n---\n","mtime":1400174380000,"source":"source/_posts/test.md"},"33f96vix4ia8mlbu":{"_id":"33f96vix4ia8mlbu","content":"title: 'something about soa'\ndate: 2014-06-02 18:31:35\ntags:\n读书笔记 大型网站技术架构 核心原理和技术分析\n---\n#@source \n-随需应变：网站的可扩展架构\n#摘要\n##关于扩展性(extensibility)和伸缩性(scalability)\n-扩展性是指基础设施不需要经常变动，应用之间有较少的依赖和耦合，对需求可以敏捷相应，是系统架构层面的开闭原则，新增需求时无需改动现有结构和代码\n-伸缩性是指通过增加或者减少自身资源的方式加强或者减少自己计算处理事务的能力，如果增减是成比例的，就被成为线性伸缩性\n##实现方式\n###利用分布式消息队列\n-优点：模块不存在直接调用，新增或者修改模块对其他模块的影响最少\n-缺点：\n\n","mtime":1401705715000,"source":"source/_posts/something-about-soa.md"}}}